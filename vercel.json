{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "VITE_STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key",
    "VITE_STRIPE_PRICE_SMALL_BUSINESS": "@stripe_price_small_business",
    "VITE_STRIPE_PRICE_BUSINESS": "@stripe_price_business"
  }
}
